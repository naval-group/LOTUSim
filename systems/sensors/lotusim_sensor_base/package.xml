<?xml version="1.0"?>
<package format="2">
  <name>lotusim_sensor_base</name>
  <version>0.0.1</version>
  <description>
    The custom lotus_sensor_system package for Lotusim.
    Custom sensors have been added and gazebo sensors have been altered to pub in ros
  </description>

  <maintainer email="malcom.neo@fe.naval-group.com">malcom</maintainer>

  <author email="malcom.neo@fe.naval-group.com">malcom</author>

  <license>EPL-2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <depend>gz-cmake3</depend>
  <depend>rclcpp</depend>
  <depend>lotusim_sensor_msgs</depend>
  <depend>lotusim_msgs</depend>
  <depend>lotusim_common</depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>